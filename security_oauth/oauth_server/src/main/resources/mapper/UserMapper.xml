<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iotmars.mapper.UserMapper">


    <select id="findByName" resultType="com.iotmars.po.SysUser">
